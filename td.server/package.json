    "chai": "^4.3.4",
    "codecov": "^3.6.5",
    "eslint": "^7.25.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "passport-local": "1.0.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "supertest": "^4.0.2"
