    "clean": "rimraf dist",
    "lint": "eslint src --fix",
    "build": "npm-run-all clean transpile"
  },
  "description": "OWASP Threat Dragon - a free, open source threat modeling tool",
  "author": {
    "name": "mike.goodwin",
    "email": "mike.goodwin@owasp.org"
  },
  "license": "Apache-2.0",
  "homepage": "https://threatdragon.github.io/",
  "repository": {
    "type": "git",
    "url": "git://github.com/owasp/threat-dragon.git"
  },
  "dependencies": {
    "bunyan": "1.8.14",
    "connect-azuretables": "1.0.20",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-bunyan-logger": "^1.3.3",
    "express-session": "^1.16.2",
    "helmet": "^3.18.0",
    "octonode": "0.9.5",
    "passport": "0.4.1",
