{
  "name": "threat-dragon-vue",
  "productName": "Threat Dragon",
  "version": "2.4.1",
  "appBundleId": "org.owasp.threatdragon",
  "private": true,
  "main": "background.js",
  "scripts": {
    "audit:vue": "npm audit --production || echo 'Vulnerabilities found - review required' && exit 0",
    "build:site": "VUE_APP_WEB_ONLY=true vue-cli-service build --no-unsafe-inline",
    "build:web": "VUE_APP_WEB_ONLY=true vue-cli-service build --no-unsafe-inline",
    "build:desktop": "PYTHON_PATH=python3 NODE_ENV=production vue-cli-service electron:build",
    "build:desktop:full": "NODE_ENV=production npm run build:site && npm run start:desktop",
    "build:desktop:simple": "ELECTRON_NODE_INTEGRATION=true VUE_APP_IS_ELECTRON=true vue-cli-service build --no-unsafe-inline --dest dist-desktop && cp src/desktop/background.js dist-desktop/background.js && cp src/desktop/simple-preload.js dist-desktop/preload.js && cp src/desktop/simple-preload.js dist-desktop/simple-preload.js && cp electron-package.json dist-desktop/package.json && mkdir -p dist-desktop/node_modules && echo 'Installing electron and dependencies in dist-desktop...' && cd dist-desktop && npm install electron@25.8.4 electron-log@5.2.4 electron-updater@6.3.9 fs-extra@11.2.0 && echo 'Dependencies installed successfully'",
    "package:desktop": "npm run build:desktop",
    "package:desktop:win": "PYTHON_PATH=python3 npm run build:desktop -- --win",
    "package:desktop:mac": "PYTHON_PATH=python3 npm run build:desktop -- --mac",
    "package:desktop:linux": "PYTHON_PATH=python3 npm run build:desktop -- --linux",
    "lint": "cd .. && npm run lint:vue",
    "logs": "node check-logs.js",
    "logs:watch": "node check-logs.js --watch",
    "make-sbom:vue": "npm-run-all -lp --aggregate-output make-sbom:xml:vue make-sbom:json:vue",
    "make-sbom:xml:vue": "npm --loglevel=error exec -- cyclonedx-npm --omit=dev --ignore-npm-errors --spec-version=1.5 --output-format=XML --output-file=sbom.xml",
    "make-sbom:json:vue": "npm --loglevel=error exec -- cyclonedx-npm --omit=dev --ignore-npm-errors --spec-version=1.5 --output-format=JSON --output-file=sbom.json",
    "clean:vue": "rimraf coverage/ cypress/ dist/ dist-desktop/ node_modules/ tests/e2e/videos/ tests/e2e/reports/ tests/e2e/screenshots/ log/ results/ sbom.json sbom.xml",
    "cypress:open": "cypress open",
    "cypress:update": "npx update-browserslist-db@latest",
    "dev:site": "VUE_APP_WEB_ONLY=true vue-cli-service serve",
    "dev:vue": "VUE_APP_WEB_ONLY=true vue-cli-service serve",
    "lint:vue": "cd .. && npm run lint:vue",
    "lint:fix:vue": "cd .. && npm run lint:fix:vue",
    "lint:desktop": "cd .. && npm run lint:desktop",
    "lint:fix:desktop": "cd .. && npm run lint:fix:desktop",
    "pretest": "npm run lint:vue && npm run lint:desktop",
    "start:vue": "cd ../.. && npm run start",
    "start:desktop": "npm run build:desktop:simple && cd dist-desktop && NODE_DEBUG=module ELECTRON_ENABLE_LOGGING=true ELECTRON_LOG_LEVEL=debug npx electron@25.8.4 .",
    "start:desktop:dev": "vue-cli-service electron:serve",
    "start:site": "pm2 --name td.vue start 'vue-cli-service serve'",
    "stop:site": "pm2 delete td.vue",
    "test:vue": "vue-cli-service test:unit --testPathIgnorePatterns tests/unit/desktop/",
    "test:single": "vue-cli-service test:unit --testMatch",
    "test:desktop": "vue-cli-service test:unit --testMatch '**/tests/unit/desktop/**/*.spec.js' --collectCoverageFrom 'src/desktop/**/*.js'",
    "test:e2e": "vue-cli-service test:e2e -C e2e.local.config.js",
    "test:e2e:desktop": "vue-cli-service test:e2e -C e2e.desktop.config.js",
    "test:e2e:local": "vue-cli-service test:e2e -C e2e.local.config.js --url http://localhost:8080/",
    "test:e2e-ci": "vue-cli-service test:e2e -C e2e.ci.config.js --browser chromium --headless --url http://localhost:3000/",
    "test:e2e-ci-smokes": "vue-cli-service test:e2e -C e2e.smokes.ci.config.js --browser chromium --headless --url http://localhost:3000/",
    "test:e2e-pr": "vue-cli-service test:e2e -C e2e.ci.config.js --browser chromium --headless --url http://localhost:8080/",
    "test:e2e-pr-smokes": "vue-cli-service test:e2e -C e2e.smokes.ci.config.js --browser chromium --headless --url http://localhost:8080/",
    "test:e2e-smokes": "browserstack-cypress run --cf browserstack.smokes.json --sync",
    "test:e2e-smokes:local": "vue-cli-service test:e2e -C e2e.smokes.local.config.js --url http://localhost:8080/",
    "test:e2e-nightly": "browserstack-cypress run --cf browserstack.nightly.json --sync",
    "test:unit": "vue-cli-service test:unit --testPathIgnorePatterns tests/unit/desktop/"
  },
  "description": "OWASP Threat Dragon - a free, open source threat modeling tool",
  "author": {
    "name": "OWASP Threat Dragon",
    "email": "jon.gadsden@owasp.org",
    "url": "https://owasp.org/www-project-threat-dragon/"
  },
  "license": "Apache-2.0",
  "homepage": "https://owasp.org/www-project-threat-dragon/",
  "buildState": "-latest",
  "repository": {
    "type": "git",
    "url": "git://github.com/OWASP/threat-dragon.git"
  },
  "bugs": {
    "email": "jon.gadsden@owasp.org",
    "url": "https://github.com/OWASP/threat-dragon/issues"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@antv/x6-plugin-clipboard": "^2.1.6",
    "@antv/x6-plugin-export": "^2.1.6",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-keyboard": "^2.2.3",
    "@antv/x6-plugin-scroller": "^2.0.10",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-stencil": "^2.1.5",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "ajv": "^8.17.1",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.30",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "is-electron": "^2.2.2",
    "mutationobserver-shim": "^0.3.7",
    "passive-events-support": "^1.1.0",
    "primevue": "^4.2.5",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue-toast-notification": "^3.1.3",
    "vue3-google-signin": "^2.0.1",
    "vuex": "^4.1.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@cyclonedx/webpack-plugin": "^3.13.0",
    "@electron/notarize": "^2.3.2",
    "@pm2/agent": ">=2.0.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^2.4.6",
    "copy-webpack-plugin": "^13.0.0",
    "cypress": "^14.2.1",
    "electron": "^25.8.4",
    "electron-builder": "^25.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-vue": "^10.0.0",
    "jest": "^27.5.1",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-loader": "^17.0.0",
    "webpack": "^5.97.1"
  },
  "overrides": {
    "libxmljs2": "0.30.1"
  },
  "electronBuilder": {
    "appId": "org.owasp.threatdragon",
    "productName": "OWASP Threat Dragon",
    "directories": {
      "output": "dist-desktop"
    },
    "files": [
      "dist-desktop/**/*",
      "electron-package.json"
    ],
    "extraMetadata": {
      "main": "background.js"
    }
  }
}
