{
  "name": "threat-dragon-vue",
  "productName": "Threat Dragon",
  "version": "2.4.1",
  "appBundleId": "org.owasp.threatdragon",
  "private": true,
  "main": "background.js",
  "scripts": {
    "preinstall": "npm config set engine-strict false && npm config set legacy-peer-deps true && npm config set strict-peer-deps false && npm config set fund false && npm config set prefer-offline true",
    "audit:vue": "npm audit --omit=dev || echo 'Vulnerabilities found - review required' && exit 0",
    "build:site": "VUE_APP_WEB_ONLY=true npx vue-cli-service build --no-unsafe-inline",
    "build:web": "VUE_APP_WEB_ONLY=true npx vue-cli-service build --no-unsafe-inline",
    "build:desktop": "node build-desktop.js",
    "build:desktop:full": "NODE_ENV=production npm run build:site && npm run start:desktop",
    "build:desktop:simple": "ELECTRON_NODE_INTEGRATION=true VUE_APP_IS_ELECTRON=true vue-cli-service build --no-unsafe-inline --dest dist-desktop && cp src/desktop/background.js dist-desktop/background.js && cp src/desktop/simple-preload.js dist-desktop/preload.js && cp src/desktop/simple-preload.js dist-desktop/simple-preload.js && cp electron-package.json dist-desktop/package.json && mkdir -p dist-desktop/node_modules && echo 'Installing electron and dependencies in dist-desktop...' && cd dist-desktop && npm install electron@25.8.4 electron-log@5.2.4 electron-updater@6.3.9 fs-extra@11.2.0 && echo 'Dependencies installed successfully'",
    "package:desktop": "npm run build:desktop",
    "package:desktop:win": "PYTHON_PATH=python3 npm run build:desktop -- --win",
    "package:desktop:mac": "PYTHON_PATH=python3 npm run build:desktop -- --mac",
    "package:desktop:linux": "PYTHON_PATH=python3 npm run build:desktop -- --linux",
    "lint": "eslint --ext .js,.vue src",
    "lint:vue": "eslint --ext .js,.vue src",
    "lint:fix:vue": "eslint --ext .js,.vue src --fix",
    "lint:desktop": "eslint --ext .js src/desktop tests/unit/desktop",
    "lint:fix:desktop": "eslint --ext .js src/desktop tests/unit/desktop --fix",
    "logs": "node check-logs.js",
    "logs:watch": "node check-logs.js --watch",
    "make-sbom:vue": "npm-run-all -lp --aggregate-output make-sbom:xml:vue make-sbom:json:vue",
    "make-sbom:xml:vue": "npm exec -- cyclonedx-npm --omit=dev --ignore-npm-errors --spec-version=1.5 --output-format=XML --output-file=sbom.xml",
    "make-sbom:json:vue": "npm exec -- cyclonedx-npm --omit=dev --ignore-npm-errors --spec-version=1.5 --output-format=JSON --output-file=sbom.json",
    "clean:vue": "rimraf coverage/ cypress/ dist/ dist-desktop/ node_modules/ tests/e2e/videos/ tests/e2e/reports/ tests/e2e/screenshots/ log/ results/ sbom.json sbom.xml",
    "cypress:open": "cypress open",
    "cypress:update": "npx update-browserslist-db@latest",
    "dev:site": "VUE_APP_WEB_ONLY=true npx vue-cli-service serve",
    "dev:vue": "VUE_APP_WEB_ONLY=true npx vue-cli-service serve",
    "pretest": "npm run lint:vue && npm run lint:desktop",
    "start:vue": "npm run build:site && npm run start:site",
    "start:desktop": "npm run build:desktop:simple && cd dist-desktop && NODE_DEBUG=module ELECTRON_ENABLE_LOGGING=true ELECTRON_LOG_LEVEL=debug npx electron@25.8.4 .",
    "start:desktop:dev": "npx vue-cli-service electron:serve",
    "start:site": "pm2 --name td.vue start 'npx vue-cli-service serve'",
    "stop:site": "pm2 delete td.vue",
    "test:vue": "npx vue-cli-service test:unit --testPathIgnorePatterns tests/unit/desktop/",
    "test:single": "npx vue-cli-service test:unit --testMatch",
    "test:desktop": "npx vue-cli-service test:unit --testMatch '**/tests/unit/desktop/**/*.spec.js' --collectCoverageFrom 'src/desktop/**/*.js'",
    "test:e2e": "npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.local.config.js",
    "test:e2e:desktop": "npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.desktop.config.js",
    "test:e2e:local": "npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.local.config.js --url http://localhost:8080/",
    "test:e2e-ci": "JEST_CONFIG=jest.e2e.config.js npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.ci.config.js --browser chromium --headless --url http://localhost:3000/",
    "test:e2e-ci-smokes": "JEST_CONFIG=jest.e2e.config.js npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.smokes.ci.config.js --browser chromium --headless --url http://localhost:3000/",
    "test:e2e-pr": "JEST_CONFIG=jest.e2e.config.js npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.ci.config.js --browser chromium --headless --url http://localhost:8080/",
    "test:e2e-pr-smokes": "JEST_CONFIG=jest.e2e.config.js npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.smokes.ci.config.js --browser chromium --headless --url http://localhost:8080/",
    "test:e2e-smokes": "JEST_CONFIG=jest.e2e.config.js npx --legacy-peer-deps browserstack-cypress run --cf browserstack.smokes.json --sync",
    "test:e2e-smokes:local": "npx --legacy-peer-deps vue-cli-service test:e2e -C e2e.smokes.local.config.js --url http://localhost:8080/",
    "test:e2e-nightly": "npx --legacy-peer-deps browserstack-cypress run --cf browserstack.nightly.json --sync",
    "test:unit": "npx vue-cli-service test:unit --testPathIgnorePatterns tests/unit/desktop/"
  },
