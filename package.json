{
  "name": "threat-dragon",
  "version": "2.4.1",
  "appBundleId": "org.owasp.threatdragon",
  "private": true,
  "scripts": {
    "preinstall": "npm config set legacy-peer-deps true",
    "install:all": "npm install --omit=optional && cd td.vue && npm install --omit=optional && cd ../td.server && npm install --omit=optional && cd ..",
    "ci:all": "npm ci && cd td.vue && npm ci && cd ../td.server && npm ci && cd ..",
    "audit": "npm-run-all -p \"audit:*\" || exit 0",
    "audit:server": "cd td.server && npm run audit:server || exit 0",
    "audit:vue": "cd td.vue && npm run audit:vue",
    "build": "npm-run-all -p \"build:server\" \"build:site\"",
    "build:desktop": "cd td.vue && npm run build:desktop",
    "build:desktop:full": "cd td.vue && npm run build:desktop:full",
    "build:desktop:simple": "cd td.vue && npm run build:desktop:simple",
    "build:server": "cd td.server && npm run build:server",
    "build:site": "cd td.vue && npm run build:site",
    "clean": "npm-run-all -p \"clean:*\"",
    "clean:files": "rimraf node_modules/ sbom.json sbom.xml",
    "clean:server": "cd td.server && npm run clean:server",
    "clean:vue": "cd td.vue && npm run clean:vue",
    "clean:lock": "npm-run-all -p \"clean:lock:*\"",
    "clean:lock:root": "npm run clean:files && rimraf package-lock.json",
    "clean:lock:server": "cd td.server && npm run clean:lock",
    "clean:lock:vue": "cd td.vue && npm run clean:lock",
    "cypress:open": "cd td.vue && npm run cypress:open",
    "cypress:update": "cd td.vue && npm run cypress:update",
    "dev": "npm-run-all --parallel dev:site dev:server:pm2",
    "dev:server": "cd td.server && npm run dev:server",
    "dev:server:pm2": "cd td.server && npm run dev:server:pm2",
    "dev:site": "cd td.vue && npm run dev:site",
    "lint": "npm-run-all lint:server lint:vue lint:desktop",
    "lint:desktop": "eslint --ext .js td.vue/src/desktop td.vue/tests/unit/desktop",
    "lint:fix": "npm run lint:fix:server && npm run lint:fix:vue && npm run lint:fix:desktop || true",
    "lint:fix:desktop": "eslint --ext .js td.vue/src/desktop td.vue/tests/unit/desktop --fix",
    "lint:fix:server": "eslint --ext .js td.server/src --fix",
    "lint:fix:vue": "eslint --ext .js,.vue td.vue/src --fix",
    "lint:server": "eslint --ext .js td.server/src",
    "lint:vue": "eslint --ext .js,.vue td.vue/src",
    "make-sbom": "npm-run-all -p make-sbom:json make-sbom:xml",
    "make-sbom:json": "npm-run-all -p make-sbom:json:server make-sbom:json:vue",
    "make-sbom:json:server": "cd td.server && npm run make-sbom:json:server",
    "make-sbom:json:vue": "cd td.vue && npm run make-sbom:json:vue",
    "make-sbom:server": "cd td.server && npm run make-sbom:server",
    "make-sbom:vue": "cd td.vue && npm run make-sbom:vue",
    "make-sbom:xml": "npm-run-all -p make-sbom:xml:server make-sbom:xml:vue",
    "make-sbom:xml:server": "cd td.server && npm run make-sbom:xml:server",
    "make-sbom:xml:vue": "cd td.vue && npm run make-sbom:xml:vue",
    "package:desktop": "cd td.vue && npm run package:desktop",
    "package:desktop:linux": "cd td.vue && npm run package:desktop:linux",
    "package:desktop:mac": "cd td.vue && npm run package:desktop:mac",
    "package:desktop:win": "cd td.vue && npm run package:desktop:win",
    "pretest": "npm-run-all pretest:server pretest:site",
    "pretest:server": "cd td.server && npm run pretest",
    "pretest:site": "cd td.vue && npm run pretest",
    "security": "npm audit && npm outdated",
    "start": "npm run build && npm-run-all --parallel start:server start:site",
    "start:desktop": "cd td.vue && npm run start:desktop",
    "start:desktop:simple": "cd td.vue && npm run start:desktop:simple",
    "start:server": "cd td.server && npm run start:server",
    "start:site": "cd td.vue && npm run start:site",
    "stop": "npm-run-all stop:site stop:server",
    "stop:server": "cd td.server && npm run stop:server",
    "stop:site": "cd td.vue && npm run stop:site",
    "test": "npm-run-all test:vue test:server",
    "test:desktop": "cd td.vue && npm run test:desktop",
    "test:server": "cd td.server && npm run test:unit",
    "test:site": "cd td.vue && npm run test:unit",
    "test:vue": "cd td.vue && npm run test:unit",
    "test:e2e": "cd td.vue && npm --legacy-peer-deps run test:e2e",
    "test:e2e:desktop": "cd td.vue && npm --legacy-peer-deps run test:e2e:desktop",
    "test:e2e:local": "cd td.vue && npm --legacy-peer-deps run test:e2e:local",
    "test:e2e-ci": "cd td.vue && npm --legacy-peer-deps run test:e2e-ci",
    "test:e2e-ci-smokes": "cd td.vue && npm --legacy-peer-deps run test:e2e-ci-smokes",
    "test:e2e-pr": "cd td.vue && npm --legacy-peer-deps run test:e2e-pr",
    "test:e2e-pr-smokes": "cd td.vue && npm --legacy-peer-deps run test:e2e-pr-smokes",
    "test:e2e-smokes": "cd td.vue && npm --legacy-peer-deps run test:e2e-smokes",
    "test:e2e-smokes:local": "cd td.vue && npm --legacy-peer-deps run test:e2e-smokes:local",
    "test:e2e-nightly": "cd td.vue && npm --legacy-peer-deps run test:e2e-nightly",
    "test:unit": "npm install @vue/cli-plugin-babel && npx vue-cli-service test:unit --testPathIgnorePatterns tests/unit/desktop/"
  },
  "keywords": [
    "threat dragon",
    "OWASP",
    "threat modeling",
    "STRIDE",
    "LINDDUN",
    "CIA",
    "DIE",
    "PLOT4ai"
  ],
  "description": "OWASP Threat Dragon - a free, open source threat modeling tool",
  "author": {
    "name": "OWASP Threat Dragon",
    "email": "jon.gadsden@owasp.org",
    "url": "https://owasp.org/www-project-threat-dragon/"
  },
  "license": "Apache-2.0",
  "homepage": "https://owasp.org/www-project-threat-dragon/",
  "repository": {
    "type": "git",
    "url": "git://github.com/OWASP/threat-dragon.git"
  },
  "bugs": {
    "email": "jon.gadsden@owasp.org",
    "url": "https://github.com/OWASP/threat-dragon/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@babel/node": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@babel/runtime": "^7.26.10",
    "@cyclonedx/cyclonedx-npm": "^2.1.0",
    "@wdio/cli": "^9.12.2",
    "babel-loader": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "dependency-cruiser": "^13.1.5",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-scope": "^8.3.0",
    "eslint-visitor-keys": "^4.2.0",
    "espree": "^10.3.0",
    "got": "^11.8.5",
    "is-mergeable-object": "^1.1.1",
    "jest": "^27.5.1",
    "libxmljs2": "0.35.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "package-json": "^8.1.0",
    "pm2": "^5.4.3",
    "rimraf": "^3.0.2",
    "semver": "^7.7.1",
    "typescript": "^5.8.3",
    "update-notifier": "^6.0.2",
    "ws": "^8.18.0",
    "yorkie": "npm:husky@^7.0.0",
    "eslint-plugin-es-x": "^8.6.1",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-promise": "^7.2.1"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "http-errors": "^2.0.0"
  },
  "overrides": {
    "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1",
    "glob-parent@<5.1.2": ">=5.1.2",
    "glob": "^7.2.3",
    "micromatch": ">=4.0.8",
    "minimatch": "^3.1.2",
    "minimatch@<3.0.5": ">=3.0.5",
    "rimraf": "^3.0.2",
    "tar-fs": ">=3.0.8",
    "@vue/vue3-jest": "27.0.0",
    "@typescript-eslint/visitor-keys": "^4.33.0"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "jest": "27.5.1"
  }
}
