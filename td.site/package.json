{
  "name": "owasp-threat-dragon-site",
  "version": "1.4.0",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean templates bundle",
    "bundle": "webpack",
    "clean": "rimraf ../dist",
    "pretest": "jshint --verbose --show-non-errors src",
    "test": "npm-run-all build test:firefox",
    "test:phantomjs": "karma start --single-run --browsers PhantomJS",
    "test:firefox": "karma start --single-run --browsers Firefox",
    "test:chrome": "karma start --single-run --browsers Chrome",
    "test:ie": "karma start --single-run --browsers IE",
    "test:local": "npm-run-all pretest test:chrome test:ie test:phantomjs test:firefox",
    "citest": "karma start --single-run false --browsers Firefox",
    "templates": "node_modules/.bin/nghtml2js -s es2015 -p src/app/core/ -f 'src/app/core/**/*.html' -m templates -o src/app/core/templates.js"
  },
  "description": "OWASP Threat Dragon - a free, open source threat modeling tool",
  "author": {
    "name": "mike.goodwin",
    "email": "mike.goodwin@owasp.org"
  },
  "license": "Apache-2.0",
  "homepage": "https://threatdragon.github.io/",
  "repository": {
    "type": "git",
    "url": "git://github.com/owasp/threat-dragon.git"
  },
  "dependencies": {
    "angular": "1.8.2",
    "angular-animate": "1.8.2",
    "angular-route": "1.8.2",
    "angular-ui-bootstrap": "2.5.6",
    "angular-xeditable": "^0.10.0",
    "bootstrap": "3.4.1",
    "font-awesome": "4.7.0",
    "hotkeys-js": "^3.7.6",
    "jointjs": "^2.2.1",
    "jquery": "^3.4.1",
    "json-rules-engine": "3.1.0",
    "lodash": "^4.17.19",
    "pug": "^3.0.1",
    "pug-bootstrap": "0.0.16",
    "rework-npm-cli": "^0.1.3",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "angular-mocks": "1.8.0",
    "angular-template-cache": "^1.3.3",
    "browserify": "^16.3.0",
    "browserify-css": "^0.15.0",
    "browserify-istanbul": "3.0.1",
    "browserify-ng-html2js": "1.3.0",
    "codecov": "^3.6.5",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "istanbul": "0.4.5",
    "jasmine": "^2.99.0",
    "jasmine-core": "^3.4.0",
    "jasmine-jquery": "2.1.1",
    "jasmine-node": "^3.0.0",
    "jasmine-spec-reporter": "4.2.1",
    "jshint": "^2.10.2",
    "karma": "^3.1.4",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "1.5.0",
    "karma-spec-reporter": "0.0.32",
    "karma-threshold-reporter": "^0.1.15",
    "karma-xml-reporter": "0.1.4",
    "mini-css-extract-plugin": "^1.5.0",
    "mockery": "2.1.0",
    "npm-run-all": "^4.1.5",
    "phantomjs-polyfill-find": "0.0.1",
    "phantomjs-prebuilt": "2.1.16",
    "requirejs": "^2.3.6",
    "rimraf": "^2.7.1",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0"
  }
}
